{
	"name": "fitr-svg-text",
	"version": "0.0.4",
	"description": "Fits text of any number of characters into provided dimensions and position within svg.",
	"main": "dist/index.js",
	"scripts": {
		"start": "npm run storybook",
		"storybook": "start-storybook -p 9001 -c .storybook",
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean-dist": "rm -rf ./dist",
		"build": "yarn run clean-dist && NODE_ENV=production `yarn bin`/babel ./src --out-dir ./dist"
	},
	"keywords": [
		"react-component",
		"fittext",
		"textsize",
		"svgtext",
		"svg",
		"react"
	],
	"author": "Ivan Maksimovic",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ivnmaksimovic/fitr-svg-text"
	},
	"devDependencies": {
		"@storybook/react": "^3.4.5",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"react": "^16.3.2",
		"react-dom": "^16.3.2"
	},
	"peerDependencies": {
		"react": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0",
		"react-dom": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0"
	},
	"dependencies": {
		"prop-types": "^15.6.1"
	}
}

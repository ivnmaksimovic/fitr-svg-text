{
	"name": "fitr-svg-text",
	"version": "1.0.0",
	"description": "Fits text of any number of characters into provided dimensions and position within svg.",
	"main": "dist/index.js",
	"scripts": {
		"start": "npm run storybook",
		"storybook": "start-storybook -p 9001 -c .storybook",
		"static-docs": "build-storybook -c .storybook -o docs",
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean-dist": "rm -rf ./dist",
		"build": "yarn run clean-dist && NODE_ENV=production `yarn bin`/babel ./src --out-dir ./dist"
	},
	"keywords": [
		"react-component",
		"fittext",
		"textsize",
		"svgtext",
		"svg",
		"react"
	],
	"author": "Ivan Maksimovic",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ivnmaksimovic/fitr-svg-text"
	},
	"devDependencies": {
		"@babel/cli": "^7.1.2",
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"@babel/preset-react": "^7.0.0",
		"@storybook/addon-info": "4.0.0-rc.0",
		"@storybook/addon-knobs": "4.0.0-rc.0",
		"@storybook/addons": "4.0.0-rc.0",
		"@storybook/react": "4.0.0-rc.0",
		"babel-loader": "^8.0.4",
		"react": "^16.3.2",
		"react-dom": "^16.3.2"
	},
	"peerDependencies": {
		"react": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0",
		"react-dom": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0"
	},
	"dependencies": {
		"prop-types": "^15.6.1"
	}
}
